package Exam;

public class TestLoop {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int x = 5;
		
		do {
			System.out.println("do ... while " + x--);
		} while (false);
		while(x > 5) {
			System.out.println("While ...");
		}
		
		System.out.println("End of Program " + --x);

	}
}
