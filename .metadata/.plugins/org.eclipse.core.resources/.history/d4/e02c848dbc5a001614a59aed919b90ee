package Exam;

public class Demo {
	
	final static int i = 2;
	public static int j = 0;

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		for (int k = 0; k < 3; k++) {
			switch (k) {
			
			case i: i=i+1;
			break;
			case i-1: System.out.print("i = " + i);
			break;
			default: System.out.print("j = " + j);
			}
		}
	}
}
